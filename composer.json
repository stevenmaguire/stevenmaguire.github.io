{
    "require":
    {
        "php": "^8.2",
        "tightenco/jigsaw": "^1.7",
        "squizlabs/php_codesniffer": "^3.7",
        "nesbot/carbon": "^2.68",
        "samdark/sitemap": "^2.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload":
    {
        "psr-4":
        {
            "App\\": "app/",
            "Components\\": "app/Components/"
        }
    },
    "require-dev":
    {
        "laravel/pint": "^1.2"
    },
    "scripts":
    {
        "lint":
        [
            "vendor/bin/pint",
            "vendor/bin/phpcs . --standard=./phpcs.xml"
        ]
    }
}
